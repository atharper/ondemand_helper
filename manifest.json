{
  "name": "Bitbucket Builds",
  "version": "0.0.w",
  "manifest_version": 2,
  "description": "Injecting build column into bitbucket and connect pages",
  "homepage_url": "http://caselle.com",
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ],
    "persistent": true
  },
  "content_scripts": [ {
    "matches": ["https://bitbucket.org/caselledevops/connectmastergit/commits*", "https://github.com/caselle/Connect/commits*"],
    "js": ["jquery.min.js", "inject.js"]
  } ],
  "browser_action": {
    "default_title": "Inject javascript"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs"
  ]
}
